package am;

import java.util.*;

public class Sub01 {
	public static void run() {
		ArrayList<Car> cars = new ArrayList<Car>();
	
		Car car1 = new Car();
		car1.company="현대";
		car1.model="그렌져";
		car1.color="검정";
		car1.distance=42786;
		car1.date="2021-06";
//		car1.print();
		
		Car car2 = new Car();
		car1.company="쌍용";
		car1.model="티볼리";
		car1.color="파랑";
		car1.distance=85122;
		car1.date="2020-07";
//		car1.print();
		
		cars.add(car1);
		cars.add(car2);
		
		for(Car c: cars) {
			c.print();
		}
	}
}
